<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.sapteh.controller.MainController">
   <children>
      <TextField fx:id="loginTxt" layoutX="218.0" layoutY="93.0">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="passwordTxt" layoutX="218.0" layoutY="155.0">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>

      <Label layoutX="90.0" layoutY="99.0" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="90.0" layoutY="161.0" text="Password">
         <font>
            <Font size="18.0" />
         </font>
      </Label>

      <Button layoutX="450.0" layoutY="318.0" mnemonicParsing="false" onAction="#inputButton" text="Войти">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="75.0" layoutY="318.0" mnemonicParsing="false" onAction="#exitButton" text="Выход">
         <font>
            <Font size="18.0" />
         </font>
      </Button>

      <Label fx:id="alertText" layoutX="100" layoutY="200" textAlignment="CENTER">
         <font>
            <Font size="18" />
         </font>
      </Label>
   </children>
</AnchorPane>
